{
  "expo": {
    "name": "球圣体育",
    "slug": "kok-score-app",
    "description": "体育比分，足球、篮球",
    "owner": "kokfootball002",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/xf-logo.png",
    "scheme": "kokscore",
    "splash": {
      "image": "./src/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
