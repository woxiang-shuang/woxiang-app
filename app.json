    ],
    "ios": {
      "bundleIdentifier": "com.kokscorefrontapp",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.kokscorefrontapp",
      "versionCode": 1
    },
    "entryPoint": "node_modules/expo/AppEntry.js"
  }
}
